{
  "name": "gdn_ktn_bund",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "split": "node scripts/processData.js",
    "build-data": "node scripts/buildAvailableData.js",
    "convert-xml": "node scripts/convertXmlToCsv.js",
    "fetch-atlas": "node scripts/fetchAtlasData.js",
    "dev": "vite",
    "build": "run-s build-data type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "tests": "run-p test:unit test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e:html": "playwright test --reporter=html",
    "test:e2e": "playwright test --reporter=line",
    "test": "run-s test:coverage test:e2e",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "pre-commit": "run-s lint format test:unit build test:e2e",
    "deploy": "npm run build && wrangler deploy"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "@tailwindcss/vite": "^4.1.8",
    "chart.js": "4.4.9",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.5.1",
    "@intlify/eslint-plugin-vue-i18n": "^4.0.1",
    "@playwright/test": "^1.53.0",
    "@primevue/auto-import-resolver": "^4.3.5",
    "@tailwindcss/postcss": "^4.1.10",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.0",
    "@types/papaparse": "^5.3.16",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/eslint-plugin": "^1.2.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@xmldom/xmldom": "^0.9.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.28.0",
    "eslint-plugin-oxlint": "^1.0.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.0.0",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.19.2"
  }
}
